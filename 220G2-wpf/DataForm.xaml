<Window x:Class="_220G2_wpf.DataForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_220G2_wpf"
        mc:Ignorable="d"
        Title="DataForm" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="dG" d:ItemsSource="{d:SampleData ItemCount=5}" MouseDoubleClick="dG_MouseDoubleClick" Margin="0,44,0,0"/>
        <Button Content="Back" HorizontalAlignment="Left" Margin="46,18,0,0" VerticalAlignment="Top" Click="Button_Click"/>

    </Grid>
</Window>
